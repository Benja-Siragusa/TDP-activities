Algoritmo sin_titulo
	definir n1, n2 como enteros;
	Escribir "dame un numero";
	Leer n1;
	Escribir "dame otro numero";
	Leer n2;
	Si n1>n2 Entonces
		Escribir "el menor es el ",n2;
	SiNo
		Si n1=n2 Entonces
			Escribir "ambos numeros son iguales";
		SiNo
			Escribir "el menor es ",n1;
		FinSi
	FinSi
FinAlgoritmo
